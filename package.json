{
  "name": "delightmom-scratch-vm",
  "version": "0.2.16",
  "description": "Virtual Machine for Delightmom",
  "author": "Massachusetts Institute of Technology",
  "license": "BSD-3-Clause",
  "main": "./dist/node/delightmom-scratch-vm.js",
  "browser": "./src/index.js",
  "scripts": {
    "build": "webpack --progress --colors --bail",
    "coverage": "tap ./test/{unit,integration}/*.js --coverage --coverage-report=lcov",
    "deploy": "touch playground/.nojekyll",
    "docs": "jsdoc -c .jsdoc.json",
    "i18n:src": "mkdirp translations/core && format-message extract --out-file translations/core/en.json src/extensions/**/**.js src/devices/**/**.js",
    "i18n:push": "tx-push-src openblock-editor extensions translations/core/en.json",
    "lint": "eslint . && format-message lint src/**/*.js",
    "start": "webpack-dev-server",
    "tap": "",
    "tap:unit": "",
    "tap:integration": "",
    "test": "npm run lint && npm run docs && npm run tap",
    "watch": "webpack --progress --colors --watch",
    "prepublish": "in-publish && npm run build || not-in-publish",
    "link": "npm link delightmom-scratch-blocks delightmom-scratch-l10n"
  },
  "dependencies": {
    "@tensorflow-models/knn-classifier": "^1.2.2",
    "@tensorflow/tfjs": "0.13.5",
    "@vernier/godirect": "1.5.0",
    "arraybuffer-loader": "^1.0.6",
    "atob": "2.1.2",
    "btoa": "1.2.1",
    "cache-lib": "0.0.5",
    "canvas-toBlob": "1.0.0",
    "decode-html": "2.0.0",
    "diff-match-patch": "1.0.4",
    "dom-serializer": "0.1.0",
    "format-message": "6.2.1",
    "htmlparser2": "3.10.0",
    "immutable": "3.8.1",
    "jszip": "^3.1.5",
    "loadjs": "^4.2.0",
    "lodash": "^4.17.21",
    "minilog": "3.1.0",
    "node-fetch": "^2.6.1",
    "scratch-parser": "5.0.0",
    "scratch-sb1-converter": "0.2.7",
    "scratch-translate-extension-languages": "0.0.20191118205314",
    "sweetalert": "^2.1.2",
    "text-encoding": "0.7.0",
    "uuid": "^8.3.2",
    "worker-loader": "^1.1.1"
  },
  "peerDependencies": {
    "scratch-svg-renderer": "^0.2.0-prerelease"
  },
  "devDependencies": {
    "@babel/core": "7.13.10",
