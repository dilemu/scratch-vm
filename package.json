    "babel-loader": "8.2.2",
    "callsite": "1.0.0",
    "copy-webpack-plugin": "4.5.4",
    "delightmom-scratch-blocks": "latest",
    "delightmom-scratch-l10n": "latest",
    "docdash": "1.2.0",
    "eslint": "5.3.0",
    "eslint-config-scratch": "5.1.0",
    "expose-loader": "0.7.5",
    "file-loader": "2.0.0",
    "format-message-cli": "6.2.0",
    "in-publish": "2.0.1",
    "jsdoc": "3.6.6",
    "json": "^9.0.4",
    "lodash.defaultsdeep": "4.6.1",
    "pngjs": "3.3.3",
    "scratch-audio": "0.1.0-prerelease.20200528195344",
    "scratch-render": "0.1.0-prerelease.20211028200436",
    "scratch-render-fonts": "1.0.0-prerelease.20210401210003",
    "scratch-storage": "1.3.3",
    "scratch-svg-renderer": "0.2.0-prerelease.20210727023023",
    "script-loader": "0.7.2",
    "stats.js": "0.17.0",
    "tap": "12.0.1",
    "tiny-worker": "2.3.0",
